#!name = ScriptHub
#!desc = https://script.hub
#!author = 小白脸、xream、keywos、CKYB

[General]
force-http-engine-hosts = script.hub, *.script.hub

[Script]
http-request ^https?:\/\/script\.hub\/($|edit\/|reload) script-path=https://raw.githubusercontent.com/Script-Hub-Org/Script-Hub/main/script-hub.js, timeout=30, tag=ScriptHub

http-request ^https?:\/\/script\.hub\/file\/_start_\/.+type=qx-rewrite script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/QX-Rewrite-Parser.js, timeout=30, tag=ScriptHub

http-request ^https?:\/\/script\.hub\/file\/_start_\/.+type=loon-plugin script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/Loon-Rewrite-Parser.js, timeout=30, tag=ScriptHub

http-request ^https?:\/\/script\.hub\/file\/_start_\/.+type=surge-module script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/Surge-Rewrite-Parser.js, timeout=30, tag=ScriptHub

http-request ^https?:\/\/script\.hub\/file\/_start_\/.+type=rule-set script-path=https://github.com/Script-Hub-Org/Script-Hub/raw/main/rule-parser.js, timeout=30, tag=ScriptHub

http-request ^https?:\/\/script\.hub\/convert\/ script-path=https://raw.githubusercontent.com/Script-Hub-Org/Script-Hub/main/script-converter.js, timeout=30, tag=ScriptHub

[MITM]
hostname = script.hub, *.script.hub
